if (trail) 
{
    trail_rate -=1;
    if (trail_rate&lt;0)
    {
        trail_rate=15;
        footstep_sound = choose(snd_footstep1,snd_footstep2,snd_footstep3,snd_footstep4,snd_footstep5);
        
        if (trail_side=1){trail_side=-1;}else{trail_side+=1;}
        if (trail_type='mud')
        {
            
            instance_create(x+trail_side,y+120,obj_footprint_mud);
        }
        if (trail_type='water')
        {
            
            instance_create(x+trail_side,y+120,obj_footprint_water);
            audio_play_sound_on(obj_footprint_water.footstep_water_em, footstep_sound,false,1);
        }
        if (trail_duration&lt;0) {trail = false;}
    }
}
}

//Vspeed
